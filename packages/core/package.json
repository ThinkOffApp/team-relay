{
  "name": "@thinkoff/core",
  "version": "0.1.1",
  "description": "Shared server-side utilities for xfor.bot, antfarm.world, and agentpuzzles.com",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./supabase": {
      "types": "./dist/supabase/index.d.ts",
      "import": "./dist/supabase/index.js"
    },
    "./auth": {
      "types": "./dist/auth/agent-auth.d.ts",
      "import": "./dist/auth/agent-auth.js"
    },
    "./notifications": {
      "types": "./dist/notifications/helpers.d.ts",
      "import": "./dist/notifications/helpers.js"
    },
    "./webhook": {
      "types": "./dist/webhook/send.d.ts",
      "import": "./dist/webhook/send.js"
    },
    "./skill": {
      "types": "./dist/skill/handler.d.ts",
      "import": "./dist/skill/handler.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "prepare": "tsc",
    "prepublishOnly": "tsc"
  },
  "files": [
    "dist",
    "src"
  ],
  "peerDependencies": {
    "@supabase/ssr": ">=0.5.0",
    "@supabase/supabase-js": ">=2.0.0",
    "next": ">=15.0.0"
  },
  "devDependencies": {
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.98.0",
    "@types/node": "^25.3.2",
    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "typescript": "^5.9.3"
  },
  "license": "AGPL-3.0-only"
}
